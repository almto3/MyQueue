<resources>

    <string name="app_name">MyQueue</string>


    <!--
      Naming convention for strings in this file
      screenx_stringName
      where x is the first instance where the string in mentioned
      and stringName doesn't have any - or _
      -->

    <string name="screen1_headerText">Please select the services you would like to see movies and TV shows from:</string>

    <string name="screen1_hboGo">HBO GO</string>
    <string name="screen1_hulu">Hulu</string>
    <string name="screen1_netflix">Netflix</string>
    <string name="screen1_starz">Starz</string>
    <string-array name="screen1_services">
        <item>@string/screen1_hboGo</item>
        <item>@string/screen1_hulu</item>
        <item>@string/screen1_netflix</item>
        <item>@string/screen1_starz</item>
    </string-array>

</resources>
